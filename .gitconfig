[alias]
    br = branch
    co = checkout
    ci = commit
    st = status
    pl = pull
    ls = log --stat --graph
    mylog = log --stat --author=xiangce --since=1.week
    last = log -1 HEAD
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
[user]
    name = Xiangce Liu
    email = xiangceliu@redhat.com
[core]
    editor = vim
